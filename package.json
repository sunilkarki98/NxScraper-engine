{
    "name": "@nx-scraper/monorepo",
    "version": "2.0.0",
    "private": true,
    "type": "module",
    "workspaces": [
        "packages/*",
        "packages/scrapers/*"
    ],
    "scripts": {
        "build": "npm run build --workspaces --if-present",
        "test": "npm run test --workspaces --if-present",
        "test:unit": "npm run test:unit --workspaces --if-present",
        "test:integration": "npm run test:integration --workspaces --if-present",
        "dev": "npm run dev -w @nx-scraper/core",
        "start": "npm run start -w @nx-scraper/core"
    },
    "devDependencies": {
        "typescript": "^5.7.2"
    },
    "overrides": {
        "glob": "^10.4.5",
        "rimraf": "^5.0.0",
        "inflight": "^1.0.6",
        "swagger-jsdoc": {
            "glob": "^10.4.5"
        },
        "puppeteer-extra-plugin-user-preferences": {
            "puppeteer-extra-plugin-user-data-dir": {
                "rimraf": "^5.0.0"
            }
        },
        "puppeteer-extra-plugin-user-data-dir": {
            "rimraf": "^5.0.0"
        }
    }
}