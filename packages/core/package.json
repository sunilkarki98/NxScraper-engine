{
    "name": "@nx-scraper/core",
    "version": "2.0.0",
    "type": "module",
    "description": "Clean, scalable web scraping orchestrator with plugin architecture",
    "main": "dist/src/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc -b",
        "start": "node dist/src/index.js",
        "test": "vitest run",
        "test:unit": "vitest run --config=../../vitest.config.ts",
        "test:integration": "vitest run --config=../../vitest.integration.config.ts",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:run": "vitest run",
        "test:integration:run": "vitest run --config=../../vitest.integration.config.ts",
        "keys:create": "tsx scripts/manage-api-keys.ts create",
        "keys:list": "tsx scripts/manage-api-keys.ts list",
        "keys:revoke": "tsx scripts/manage-api-keys.ts revoke",
        "keys:help": "tsx scripts/manage-api-keys.ts help"
    },
    "dependencies": {
        "@googlemaps/google-maps-services-js": "^3.3.42",
        "@nx-scraper/google-places": "*",
        "@nx-scraper/shared": "*",
        "@sentry/node": "^10.29.0",
        "@sentry/profiling-node": "^10.29.0",
        "bullmq": "^5.66.0",
        "compression": "^1.8.1",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^5.2.1",
        "express-rate-limit": "^8.2.1",
        "express-validator": "^7.3.1",
        "helmet": "^8.1.0",
        "pino-pretty": "^12.0.0",
        "piscina": "^5.1.4",
        "prom-client": "^15.1.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "undici": "^7.16.0"
    },
    "peerDependencies": {
        "@nx-scraper/google-scraper": "*",
        "@nx-scraper/ultra-scraper": "*",
        "@nx-scraper/universal-scraper": "*"
    },
    "peerDependenciesMeta": {
        "@nx-scraper/google-scraper": {
            "optional": true
        },
        "@nx-scraper/ultra-scraper": {
            "optional": true
        },
        "@nx-scraper/universal-scraper": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/compression": "^1.8.1",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^20.19.25",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@vitest/coverage-v8": "^4.0.15",
        "tsx": "^4.21.0",
        "typescript": "^5.7.2",
        "vitest": "^4.0.15"
    },
    "overrides": {
        "glob": "^10.0.0",
        "rimraf": "^5.0.0"
    }
}
